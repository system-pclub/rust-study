ata0-master: type=disk, path="build/harddrive.bin", mode=flat
boot: disk
com1: enabled=1, mode=file, dev=build/serial.log
cpu: model=corei7_haswell_4770
megs: 1024

magic_break: enabled=1
display_library: x, options="gui_debug"

log: -
debug: action=ignore
info: action=report
error: action=report
panic: action=ask
debugger_log: -
